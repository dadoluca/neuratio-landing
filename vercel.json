{
  "functions": {
    "dist/index.js": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/public/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public"
}